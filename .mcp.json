{
  "mcpServers": {
    "b00t-mcp": {
      "args": [
        "--stdio"
      ],
      "command": "b00t-mcp",
      "env": {}
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "bunx"
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx"
    },
    "rust-crate-docs-docker": {
      "args": [
        "run",
        "--rm",
        "-i",
        "ghcr.io/promptexecution/rust-cargo-docs-rag-mcp:latest",
        "stdio"
      ],
      "command": "docker"
    },
    "taskmaster-ai": {
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "command": "bunx",
      "env": {}
    }
  }
}
